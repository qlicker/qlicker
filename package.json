{
  "name": "qlicker",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "lint": "node_modules/.bin/standard",
    "fix": "node_modules/.bin/standard --fix",
    "test:app-watch": "meteor test --full-app --driver-package practicalmeteor:mocha",
    "test:app-coverage": "node_modules/.bin/spacejam test --full-app --driver-package practicalmeteor:mocha --port 4099 --coverage 'out_lcovonly out_coverage out_html in_coverage' --xunit-out $CIRCLE_TEST_REPORTS/full-app-results.xml",
    "test:unit-watch": "meteor test --driver-package practicalmeteor:mocha",
    "test:unit-coverage": "node_modules/.bin/spacejam test --driver-package practicalmeteor:mocha --port 4099 --coverage 'out_lcovonly out_coverage out_html' --xunit-out $CIRCLE_TEST_REPORTS/unit-results.xml",
    "test:unit-no-coverage": "node_modules/.bin/spacejam test --driver-package practicalmeteor:mocha --port 4099",
    "test:all": "npm run test:unit-coverage && npm run test:app-coverage"
  },
  "dependencies": {
    "babel-eslint": "^7.1.1",
    "babel-runtime": "^6.18.0",
    "bcrypt": "^0.8.7",
    "datalib": "^1.7.3",
    "draft-js": "^0.10.0",
    "draft-js-export-html": "^0.5.2",
    "draftjs-to-html": "^0.6.0",
    "dropzone": "^4.3.0",
    "eslint": "^3.11.1",
    "immutable": "^3.8.1",
    "jquery": "^2.2.0",
    "jshint": "^2.9.4",
    "meteor-node-stubs": "~0.2.0",
    "react": "^15.4.1",
    "react-addons-pure-render-mixin": "^15.4.1",
    "react-addons-test-utils": "^15.4.2",
    "react-addons-transition-group": "^15.4.2",
    "react-dom": "^15.4.1",
    "react-drag-sortable": "^1.0.1",
    "react-mounter": "^1.2.0",
    "react-tag-input": "^4.6.2",
    "react-tools": "^0.13.3",
    "recharts": "^0.21.0",
    "syntastic-react": "^1.0.3",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "codecov": "^1.0.1",
    "enzyme": "^2.7.0",
    "eslint": "^3.14.0",
    "eslint-plugin-react": "^6.9.0",
    "ignore-styles": "^5.0.1",
    "sinon": "^1.17.7",
    "spacejam": "https://github.com/serut/spacejam/tarball/windows-suppport-rc4",
    "standard": "^8.6.0"
  },
  "standard": {
    "globals": [
      "Router",
      "Meteor",
      "Accounts",
      "it",
      "describe",
      "confirm",
      "alertify"
    ]
  }
}
